            android:layout_height="wrap_content"
            android:layout_alignTop="@id/line3String"
            android:layout_toRightOf="@id/line3String"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/line4String"
            style="@style/wizard_install_satellite_fullscan_npanel_col01_row01_text_style"
