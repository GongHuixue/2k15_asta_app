<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/easyPQRelLayout"
    android:layout_width="147dp"
    android:layout_height="fill_parent"
    android:gravity="center_vertical|center_horizontal"
    android:background="@org.droidtv.ui.tvwidget2k15:drawable/panels_divider_left_transparent_bg"	>
   
	 <ImageView
        android:id="@+id/divider"
        android:layout_width="@dimen/setup_settings_npanel_list02_panels_divider_width"
        android:layout_height="378.5dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        />
		
    <TextView
        android:id="@+id/textView1"
        android:layout_width="@dimen/setup_settings_npanel_column03_image02_text_width"
        android:layout_height="@dimen/setup_settings_npanel_column03_image02_text_height"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="67dp"
        style="@style/setup_settings_npanel_column03_image01_text_style"
        android:text="@org.droidtv.ui.strings:string/MAIN_MINIMUM"
		android:visibility="gone"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="@dimen/setup_settings_npanel_column03_image02_text_width"
        android:layout_height="@dimen/setup_settings_npanel_column03_image02_text_height"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="170.5dp"
        style="@style/setup_settings_npanel_column03_image02_text_style"
        android:text="@org.droidtv.ui.strings:string/MAIN_MAXIMUM"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="102.5dp"
		android:gravity="center_horizontal" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="2dp"
        android:gravity="center_horizontal" />

    <ImageView
		android:id="@+id/horiz_divider"
        android:layout_width="@dimen/setup_settings_npanel_column03_panels_horizontal_divider_01_width"
        android:layout_height="1dp"
        android:background="@org.droidtv.ui.tvwidget2k15:drawable/panels_horizontal_divider"
        android:layout_marginTop="193.5dp"
        android:layout_centerHorizontal="true"
		android:gravity="center_horizontal"
        android:visibility="gone" />
		
    <TextView
        android:id="@+id/textView3"
        android:layout_width="@dimen/setup_settings_npanel_column03_description_text_width"
		android:layout_height="@dimen/setup_settings_npanel_column03_description_text_height"
		android:layout_marginLeft="@dimen/setup_hint_description_marginLeft_orig"
		android:layout_marginTop="@dimen/sat_setup_hint_description_marginTop_orig"
		android:gravity="left"
		style="@style/setup_settings_easypq_description_hint_text"
        />
        
</RelativeLayout>
