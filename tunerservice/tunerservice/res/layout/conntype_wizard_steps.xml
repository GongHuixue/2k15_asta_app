        android:layout_height="wrap_content" >
    </org.droidtv.euinstallersat.wizard.settings.connectionTypeWizard.screen.UniCableUBNScreenTuner2>
    
    
    <org.droidtv.euinstallersat.wizard.settings.connectionTypeWizard.screen.UniCableUBFScreenTuner2
        android:id="@+id/unicableubf2"
        android:tag="@org.droidtv.ui.strings:string/MAIN_VB_SAT_AUTOSTORE_UB_FREQUENCY_TUNER_2"
