            android:layout_marginLeft="@dimen/wizard_install_satellite_fullscan_npanel_col01_row01_text_x1"
            android:layout_marginTop="7dp"
            android:visibility="invisible"
            android:width="400dp" />

        <TextView
            android:id="@+id/line3Value"
            style="@style/wizard_install_satellite_fullscan_npanel_col01_row02_unit01_text_style"
