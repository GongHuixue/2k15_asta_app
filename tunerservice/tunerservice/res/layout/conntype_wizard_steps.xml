        android:layout_height="wrap_content" >
    </org.droidtv.euinstallersat.wizard.settings.connectionTypeWizard.screen.UniCableUBFScreenTuner2>
    
    <org.droidtv.euinstallersat.wizard.settings.connectionTypeWizard.screen.LNBSelectionScreen
        android:id="@+id/lnbselection"
        android:tag="@org.droidtv.ui.strings:string/MAIN_LNB_SELECTION"
